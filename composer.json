{
  "name": "onyedika-anagha/raveflow",
  "description": "A Laravel package for seamless integration with Flutterwave Rave Payment Gateway",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Onyedika Anagha",
      "email": "paschalanagha@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^9.0 || ^10.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "OnyedikaAnagha\\RaveFlow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OnyedikaAnagha\\RaveFlow\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OnyedikaAnagha\\RaveFlow\\RaveFlowServiceProvider"
      ],
      "aliases": {
        "RaveFlow": "OnyedikaAnagha\\RaveFlow\\Facades\\RaveFlow"
      }
    }
  }
}
